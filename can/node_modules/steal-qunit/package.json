{
  "name": "steal-qunit",
  "version": "0.0.2",
  "description": "A bower package for QUnit and StealJS",
  "main": "steal-qunit",
  "system": {
    "npmIgnore": {
      "bower": true,
      "grunt": true,
      "grunt-cli": true,
      "grunt-contrib-copy": true,
      "grunt-contrib-watch": true,
      "steal": true,
      "steal-tools": true
    },
    "meta": {
      "./qunit": {
        "format": "global",
        "exports": "QUnit",
        "deps": [
          "./add-dom"
        ]
      }
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitovi/steal-qunit.git"
  },
  "keywords": [
    "StealJS",
    "QUnit"
  ],
  "author": {
    "name": "Bitovi"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bitovi/steal-qunit/issues"
  },
  "homepage": "https://github.com/bitovi/steal-qunit",
  "devDependencies": {
    "steal": "bitovi/steal#npm",
    "steal-tools": "bitovi/steal-tools#npm",
    "bower": "1.3.8",
    "grunt": "~0.4.1",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-copy": "~0.5.0",
    "grunt-contrib-watch": "~0.3.0"
  },
  "readme": "# steal-qunit\n\nThis provides a bower installable QUnit that fires off all tests \nwhen the app has finished loading.\n\nCurrently, it has no dependencies until [this issue](https://github.com/jquery/qunit/issues/723)\nis closed.\n",
  "readmeFilename": "README.md",
  "_id": "steal-qunit@0.0.2",
  "_from": "steal-qunit@0.0.2"
}
